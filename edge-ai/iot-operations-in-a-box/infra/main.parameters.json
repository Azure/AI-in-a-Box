{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "backendImageName": {
      "value": "${AZURE_ENV_BACKENDIMAGENAME}"
    },
    "frontendImageName": {
      "value": "${AZURE_ENV_FRONTENDIMAGENAME}"
    },
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "virtualMachineSize": {
      "value": "Standard_B4ms"
    },
    "virtualMachineName": {
      "value": "${AZURE_ENV_VIRTUALMACHINENAME}"
    },
    "adminUsername": {
      "value": "${AZURE_ENV_ADMINUSERNAME}"
    },
    "adminPasswordOrKey": {
      "value": "${AZURE_ENV_ADMINPASSWORDORKEY}"
    },
    "authenticationType": {
      "value": "${AZURE_ENV_AUTHENTICATIONTYPE}"
    },
    "arcK8sClusterName": {
      "value": "${AZURE_ENV_ARCK8SCLUSTERNAME}"
    },
    "scriptURI": {
      "value": "${AZURE_ENV_SCRIPTURI}"
    },
    "ShellScriptName": {
      "value": "${AZURE_ENV_SHELLSCRIPTNAME}"
    },
    "gitOpsAppName" : {
      "value": "${AZURE_ENV_GITOPSAPPNAME}"
    },
    "gitOpsAppNamespace" : {
      "value": "${AZURE_ENV_GITOPSAPPNAMESPACE}"
    },
    "gitOpsGitRepositoryUrl" : {
      "value": "${AZURE_ENV_GITOPSGITREPOSITORYURL}"
    },
    "gitOpsGitRepositoryBranch" : {
      "value": "${AZURE_ENV_GITOPSGITREPOSITORYBRANCH}"
    },
    "gitOpsAppPath" : {
      "value": "${AZURE_ENV_GITOPSAPPPATH}"
    }
  }
}